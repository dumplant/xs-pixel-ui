{
  "name": "xs-pixel-ui",
  "version": "0.1.3",
  "description": "pixel style ui",
  "main": "dist/index.umd.js",
  "keywords": [
    "pixel-ui",
    "pixel",
    "vue",
    "ui"
  ],
  "author": {
    "name": "zzz1024"
  },
  "files": [
    "dist",
