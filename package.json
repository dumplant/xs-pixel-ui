    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "gulp": "^3.9.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^5.1.0",
    "node-sass": "^8.0.0",
    "sass": "^1.60.0",
    "sass-loader": "^5.0.1",
    "vue-loader": "^15.10.0",
    "vue-template-compiler": "^2.6.14",
    "webpack-cli": "^5.0.1",
    "webpack-stream": "^7.0.0"
